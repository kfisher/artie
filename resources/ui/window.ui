<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="ArtieApplicationWindow" parent="GtkApplicationWindow">
        <property name="title">Artie</property>
        <property name="default-width">1080</property>
        <property name="default-height">920</property>
        <property name="titlebar">
            <object class="GtkHeaderBar">
                <child>
                    <object class="GtkStackSwitcher">
                        <property name="stack">stack</property>
                    </object>
                </child>
            </object>
        </property>
        <child>
            <object class="GtkStack" id="stack">
                <child>
                    <object class="GtkStackPage">
                        <property name="name">copy-page</property>
                        <property name="title">Copy</property>
                        <property name="child">
                            <object class="ArtieCopyPageWidget">
                            </object>
                        </property>
                    </object>
                </child>
                <child>
                    <object class="GtkStackPage">
                        <property name="name">transcode-page</property>
                        <property name="title">Transcode</property>
                        <property name="child">
                            <object class="GtkLabel">
                                <property name="label">TODO: Transcode Page</property>
                            </object>
                        </property>
                    </object>
                </child>
                <child>
                    <object class="GtkStackPage">
                        <property name="name">catalog-page</property>
                        <property name="title">Catalog</property>
                        <property name="child">
                            <object class="GtkLabel">
                                <property name="label">TODO: Catalog Page</property>
                            </object>
                        </property>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
